{
    "name": "marczhermo/search-list",
    "description": "SilverStripe SearchList",
    "type": "silverstripe-vendormodule",
    "license": "BSD-3-Clause",
    "keywords": [
        "SearchList",
        "SilverStripe",
        "search"
    ],
    "require": {
        "silverstripe/framework": "^4@dev",
        "silverstripe/vendor-plugin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "silverstripe/admin": "^1@dev",
        "silverstripe/cms": "^4@dev",
        "symbiote/silverstripe-queuedjobs": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "Marcz\\Search\\": "src/",
            "Marcz\\Search\\Tests\\": "tests/php/"
        }
    },
    "scripts": {
        "lint": "php phpcs.phar src/ tests/php/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    }
}
