{
    "name": "marczhermo/search-list",
    "description": "SilverStripe SearchList",
    "type": "silverstripe-module",
    "license": "BSD-3-Clause",
    "keywords": [
        "searchlist",
        "silverstripe",
        "search",
        "admin"
    ],
    "require": {
        "silverstripe/cms": "~3.7",
        "silverstripe/framework": "~3.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^3 || ^4 || ^5"
    },
    "extra": {
        "expose": [
            "client/dist",
            "client/lang"
        ]
    },
    "autoload": {
        "psr-4": {
            "Marcz\\Search\\": "src/",
            "Marcz\\Search\\Tests\\": "tests/php/"
        }
    },
    "scripts": {
        "lint": "php phpcs.phar src/ tests/php/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
